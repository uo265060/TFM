    if (gs.getUser().isMemberOf("Unidad técnica de la escuela de Ingeniería Informática")) {
        var group = new GlideRecord("sys_user_group");
        group.addQuery("name", "Unidad técnica de la escuela de Ingeniería Informática");
        group.query();

        if (group.next()) {
            if (current.install_status != 6 &amp;&amp; current.install_status != 11 &amp;&amp; current.install_status != 9) {
                if (group.u_edificio.name != current.location.building.name) {
                    gs.addErrorMessage('Debe seleccionar ubicaciones dentro de la Escuela de Ingeniería Informática');
                    current.setAbortAction(true);
                }
            }

        }


    } else if (gs.getUser().isMemberOf("Unidad técnica de la facultad de Biología")) {
        var group = new GlideRecord("sys_user_group");
        group.addQuery("name", "Unidad técnica de la facultad de Biología");
        group.query();

        if (group.next()) {
            if (current.install_status != 6 &amp;&amp; current.install_status != 11 &amp;&amp; current.install_status != 9) {
                if (group.u_edificio.name != current.location.building.name) {
                    gs.addErrorMessage('Debe seleccionar ubicaciones dentro de la Facultad de Biología');
                    current.setAbortAction(true);
                }
            }
        }

    } else {
		
        if (current.install_status != 6 &amp;&amp; current.install_status != 11 &amp;&amp; current.install_status != 9) {
			
            if (current.location.building == null || current.location.getAttribute("building")=="") {
                gs.addErrorMessage('Debe seleccionar ubicaciones dentro de los centros de la universidad');
                current.setAbortAction(true);
            }
        }
    }



})(current, previous);]]&gt;&lt;/script&gt;&lt;sys_class_name&gt;sys_script&lt;/sys_class_name&gt;&lt;sys_created_by&gt;lucia.admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2024-04-04 11:27:08&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;e4b5f9e21b91c65062f8ea46624bcb78&lt;/sys_id&gt;&lt;sys_mod_count&gt;21&lt;/sys_mod_count&gt;&lt;sys_name&gt;LPG Comprobar ubicación&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sys_script_e4b5f9e21b91c65062f8ea46624bcb78&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;lucia.admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2024-04-15 08:38:19&lt;/sys_updated_on&gt;&lt;template/&gt;&lt;when&gt;before&lt;/when&gt;&lt;/sys_script&gt;&lt;sys_translated_text action="delete_multiple" query="documentkey=e4b5f9e21b91c65062f8ea46624bcb78"/&gt;&lt;sys_translated_text action="INSERT_OR_UPDATE"&gt;&lt;documentkey&gt;e4b5f9e21b91c65062f8ea46624bcb78&lt;/documentkey&gt;&lt;fieldname&gt;message&lt;/fieldname&gt;&lt;language&gt;es&lt;/language&gt;&lt;sys_created_by&gt;lucia.admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2024-04-06 11:16:25&lt;/sys_created_on&gt;&lt;sys_id&gt;dd00905b1b55461018398485604bcb76&lt;/sys_id&gt;&lt;sys_mod_count&gt;0&lt;/sys_mod_count&gt;&lt;sys_updated_by&gt;lucia.admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2024-04-06 11:16:25&lt;/sys_updated_on&gt;&lt;tablename&gt;sys_script&lt;/tablename&gt;&lt;value/&gt;&lt;/sys_translated_text&gt;&lt;/record_update&gt;</payload>
